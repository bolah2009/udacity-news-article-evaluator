{
  "name": "news-article-evaluator",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server/index.js",
    "build": "webpack --config webpack.dev.js --watch",
    "build-prod": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server  --config webpack.dev.js --open",
    "format": "npx prettier  --write '**/*.{css,html,js,json,md.json,yaml,yml}'",
    "format:check": "npx prettier  --check '**/*.{css,html,js,json,md.json,yaml,yml}'",
    "lint": "npx eslint '**/*.js' --fix && npx stylelint '**/*.css' --fix",
    "lint:check": "npx eslint '**/*.js' && npx stylelint '**/*.css'",
    "html-validator": "npx html-validator-cli --quiet --file  src/index.html"
  },
  "description": "An asynchronous web app with a node server that uses Web API and user data to dynamically update the UI in a News Article Evaluator application.",
  "dependencies": {
    "aylien_textapi": "^0.7.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "1.19.1",
    "stylelint": "^12.0.0",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^1.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-fix-style-only-entries": "^0.4.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bolah2009/udacity-news-article-evaluator.git"
  },
  "keywords": [
